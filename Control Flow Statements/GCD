public class Main {
public static void main(String[] args){ 
int num1;
int num2;
Scanner sc = new Scanner(System.in);
num1 = sc.nextInt(System.in);
num2 = sc.nextInt(System.in);

while(num1 != num2) { 
if(num1>num2) { 
num1-=num2; // num1 = num1 - num2
}
else { 
num2-=num1; // num2 = num2 - num1
}

}
System.out.println("The HCF is  " +num1);

}
}
